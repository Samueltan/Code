<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
    <!-- Definition for student bean -->
    <bean id="profile" class="com.tutorialspoint.annotation.Profile"></bean>
    <bean id="student1" class="com.tutorialspoint.annotation.Student">
        <property name="name" value="Zara"/>
        <!-- try without passing age and check the result -->
        <property name="age" value="11"/>
    </bean>
    <bean id="student2"  class="com.tutorialspoint.annotation.Student">
        <property name="name" value="Nuha"/>
        <property name="age" value="2"/>
    </bean>

    <bean id="textEditor" class="com.tutorialspoint.annotation.TextEditor"></bean>
    <bean id="spellChecker" class="com.tutorialspoint.annotation.SpellChecker"></bean>
</beans>